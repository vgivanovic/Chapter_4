@article{Andra.etal2020,
  title = {Learning {{Foreign Language Vocabulary}} with {{Gestures}} and {{Pictures Enhances Vocabulary Memory}} for {{Several Months Post-Learning}} in {{Eight-Year-Old School Children}}},
  author = {Andrä, Christian and Mathias, Brian and Schwager, Anika and Macedonia, Manuela and family=Kriegstein, given=Katharina, prefix=von, useprefix=true},
